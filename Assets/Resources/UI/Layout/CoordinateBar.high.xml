<KBNMenu Name="CoordinateBar" MaxHeight="180" >
	<ContainRef Name="CoordinateRootContain" >
		<ObjToUI RefName="bgLabel">
			<ObjToUI.RealObj.mystyle.normal background="{Texture small_bar_bottom:BACKGROUND}"/>
		</ObjToUI>
		<Grid Border="{10, 10, 10, 40}" >
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="180" />
				<ColumnDefinition Width="180" />
				<ColumnDefinition Width="64" />
				<ColumnDefinition MinWidth="42"/>
				<ColumnDefinition Width="64" />
				<ColumnDefinition MinWidth="42"/>
				<ColumnDefinition Width="40" />
				<ColumnDefinition Width="180"/>
				<ColumnDefinition Width="180" />
				<ColumnDefinition Width="180" />
			</Grid.ColumnDefinitions>
			<ContainRef Grid.Col="0" Grid.Row="0">
				<ObjToUI RefName="myCityBtn" rect="{0,0,150,150}" ContainRef.deep="0" ContainRef.vFill="Center" ContainRef.hFill="Center" ContainRef.lockType="LockSize" />
				<Label NormalBackground="{Texture city_wall_icon:BUTTON}" ContainRef.deep="1" ContainRef.vFill="Center" ContainRef.hFill="Center" ContainRef.lockType="LockSize" >
					<Label.RealObj rect="{0,0,126,118}"/>
				</Label>
			</ContainRef>
			<ContainRef Grid.Col="1" Grid.Row="0">
				<ObjToUI RefName="bookmarkBtn" rect="{0,0,150,150}" ContainRef.deep="0" ContainRef.vFill="Center" ContainRef.hFill="Center" ContainRef.lockType="LockSize" />
				<Label NormalBackground="{Texture button_bookmark_big:BUTTON}" ContainRef.deep="1" ContainRef.vFill="Center" ContainRef.hFill="Center" ContainRef.lockType="LockSize" >
					<Label.RealObj rect="{0,0,126,118}"/>
				</Label>
			</ContainRef>
			
			<ContainRef Grid.Col="2" Grid.Row="0">
				<ObjToUI RefName="xLabel" FontSizeEnum="Font_36" rect="{0,0,64,64}" ContainRef.deep="0" ContainRef.vFill="Center" ContainRef.hFill="Center" ContainRef.lockType="LockSize">
					<ObjToUI.RealObj.mystyle alignment="MiddleCenter"/>
				</ObjToUI>
			</ContainRef>
			<ContainRef Grid.Col="3" Grid.Row="0">
				<ObjToUI RefName="textInputX" FontSizeEnum="Font_36" rect="{0,0,0,128}" ContainRef.deep="0" ContainRef.vFill="Center" ContainRef.hFill="Center" ContainRef.lockType="LockHeight">
                    <ObjToUI.RealObj.mystyle.normal background="{Texture type_box:DECORATION}" />
